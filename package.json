{
  "private": true,
  "name": "@msidolphin/vite-plugin",
  "version": "1.0.0",
  "scripts": {
    "test": "pnpm build && pnpm -C packages/plugin-sri/ test",
    "test:examples": "pnpm build && pnpm -C packages/plugin-sri/ test:examples",
    "build": "pnpm -C packages/plugin-sri/ build",
    "vue": "cd packages/examples/vue && pnpm i && pnpm build && pnpm preview",
    "vue:debug": "cd packages/examples/vue && pnpm i && pnpm debug",
    "react": "cd packages/examples/react && pnpm i && pnpm build && pnpm preview",
    "react:debug": "cd packages/examples/react && pnpm i && pnpm debug",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"./packages/plugin-sri/\"",
    "typecheck": "pnpm -C packages/plugin-sri/ typecheck",
    "pre-release": "pnpm changelog && pnpm format",
    "changelog": "conventional-changelog -p angular -i packages/plugin-sri/CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/cross-spawn": "^6.0.4",
    "@types/node": "^20.8.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "rollup": "^4.1.3",
    "run-p": "^0.0.0",
    "tmp-promise": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "tsup": "^7.2.0"
  }
}
